{
  "manifest_version": 3,
  "name": "Close Tabs by Domain",
  "version": "1.0",
  "description": "Close all tabs from a specific website (e.g., github.com)",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}

